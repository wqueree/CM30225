#!/bin/bash
 
# Set the account to be used for the job
#SBATCH --account=cm30225
 
# Set name of job
#SBATCH --job-name=test-parallel
#SBATCH --output=test-parallel.out
#SBATCH --error=test-parallel.err
 
# Set the number of nodes and partition
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=4
#SBATCH --partition=iteaching

 
# Set max wallclock time
#SBATCH --time=00:20:00
 
# Mail alert at start, end and abortion of execution
#SBATCH --mail-type=END
 
# Send mail to this address
#SBATCH --mail-user=wlq20@bath.ac.uk
 
# Run the application
./parallel
